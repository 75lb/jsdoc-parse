{
  "name": "jsdoc-parse",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "version": "0.1.4",
  "description": "Documented javascript source files in, doclet data out.",
  "repository": "https://github.com/75lb/jsdoc-parse.git",
  "main": "lib/jsdoc-parse.js",
  "bin": {
    "jsdoc-parse": "bin/cli.js"
  },
  "keywords": [],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "tape test/*.js",
    "lint": "jshint lib/*.js bin/*.js test/*.js; echo",
    "docs": "jsdoc2md -t jsdoc2md/README.hbs lib/jsdoc-parse.js > README.md; echo"
  },
  "dependencies": {
    "array-tools": "^1.1.5",
    "jsdoc": "jsdoc3/jsdoc",
    "object-tools": "^1.1.1"
  },
  "devDependencies": {
    "tape": "^2.13.3"
  }
}

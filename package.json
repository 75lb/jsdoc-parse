{
  "name": "jsdoc-parse",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "version": "1.2.1",
  "description": "Jsdoc-annotated source code in, JSON format documentation out.",
  "repository": "https://github.com/jsdoc2md/jsdoc-parse.git",
  "main": "index",
  "bin": {
    "jsdoc-parse": "bin/cli.js"
  },
  "license": "MIT",
  "keywords": [
    "javascript",
    "jsdoc",
    "parse",
    "document",
    "documentation"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "tape test/*.js",
    "docs": "jsdoc2md -t jsdoc2md/README.hbs -p list lib/jsdoc-parse.js > README.md; echo",
    "es5": "babel --presets babel-preset-es2015 --no-comments lib --out-dir es5"
  },
  "dependencies": {
    "ansi-escape-sequences": "^2.2.1",
    "array-tools": "^2",
    "command-line-args": "^2.1.1",
    "core-js": "^1.2.6",
    "feature-detect-es6": "^1.2.0",
    "file-set": "~0.2.1",
    "jsdoc-75lb": "^3.4.1",
    "object-tools": "^2",
    "temp-path": "^1.0.0",
    "test-value": "^1.0.1",
    "walk-back": "^1.0.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "tape": "^4"
  },
  "standard": {
    "ignore": [
      "test",
      "es5",
      "example"
    ]
  }
}

{
  "name": "jsdoc-parse",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "version": "2.0.1",
  "description": "Jsdoc-annotated source code in, JSON format documentation out.",
  "repository": "https://github.com/jsdoc2md/jsdoc-parse.git",
  "main": "index",
  "license": "MIT",
  "keywords": [
    "javascript",
    "jsdoc",
    "parse",
    "document",
    "documentation"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "tape test/*.js",
    "docs": "jsdoc2md -t jsdoc2md/README.hbs -p list lib/jsdoc-parse.js > README.md; echo",
    "es5": "babel --presets babel-preset-es2015 --no-comments lib --out-dir es5"
  },
  "dependencies": {
    "array-back": "^1.0.3",
    "core-js": "^2.4.1",
    "feature-detect-es6": "^1.3.1",
    "object-tools": "^2.0.6",
    "reduce-extract": "^1.0.0",
    "test-value": "^2.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "tape": "^4.6"
  },
  "standard": {
    "ignore": [
      "test",
      "es5",
      "example"
    ]
  }
}
